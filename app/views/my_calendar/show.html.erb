  <h1 class="float-start">Your purchased tickets:</h1>
<%= link_to "Back", :back, class: "btn btn-outline-dark float-end mt-2" %>
<hr class="clear">
  <div id="my_calendar" class="table-responsive">
    <table class="table table-dark table-striped table-hover">
        <thead>
            <tr>
                <th>Show Name</th>
                <th>Start Time</th>
                <th>Location</th>
            </tr>
        </thead>
        <tbody>
            <% current_user.attendees.each do |attendee| %>
            <tr>
                <td> <%= attendee.dog_show.name %></td>
                <td><%= attendee.dog_show.start_time %></td>
                <td><%= attendee.dog_show.location %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
  </div>